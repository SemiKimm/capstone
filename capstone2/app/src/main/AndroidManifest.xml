            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PostingActivity" />
        <activity android:name=".LostPostActivity" />
        <activity android:name=".PostListActivity" />
        <activity android:name=".LostPostListActivity" />
        <activity android:name=".GetPostListActivity" />
        <activity android:name=".SelectPostingActivity" />
        <activity android:name=".LostPostingActivity" />
        <activity android:name=".GetPostingActivity" />
        <activity android:name=".SelectSearchPostActivity" />
        <activity android:name=".SearchGetPostActivity" />
